<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" onKeyPressed="#settingsChange" onMouseMoved="#openSettings"
            prefHeight="500.0"
            prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="core.Settings">
	<left>
		<VBox prefHeight="488.0" prefWidth="256.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="6.0" left="6.0" right="4.0" top="6.0"/>
            </BorderPane.margin>
            <AnchorPane prefHeight="32.0" prefWidth="156.0">
                <CheckBox fx:id="autoSaveBox" layoutY="4.0" mnemonicParsing="false" text="AutoSave">
                    <padding>
                        <Insets right="6.0"/>
                    </padding>
                </CheckBox>
                <TextField fx:id="autoSaveTime" layoutX="97.0" layoutY="-1.0" prefHeight="30.0"
                           prefWidth="55.0"/>
                <Text layoutX="157.0" layoutY="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="seconds"/>
            </AnchorPane>
            <AnchorPane prefHeight="32.0" prefWidth="256.0"/>
		</VBox>
	</left>
</BorderPane>
