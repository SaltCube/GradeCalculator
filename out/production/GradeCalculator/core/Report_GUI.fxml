<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
			xmlns="http://javafx.com/javafx/8.0.111" fx:controller="core.Report">
	<bottom>
		<ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<buttons>
				<Button text="Save As" ButtonBar.buttonData="LEFT"/>
				<Button text="Print" ButtonBar.buttonData="RIGHT"/>
			</buttons>
			<BorderPane.margin>
				<Insets left="4.0" right="4.0"/>
			</BorderPane.margin>
		</ButtonBar>
	</bottom>
</BorderPane>
